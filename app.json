{
  "expo": {
    "name": "fitness-ready",
    "slug": "fitness-ready",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/fr-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#14191F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.svg",
        "backgroundColor": "#14191F"
      }
    },
    "web": {
      "favicon": "./assets/icons/logo.svg"
    }
  }
}
